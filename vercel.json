{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".next"
      }
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "PORTIA_API_KEY": "@portia_api_key",
    "GOOGLE_API_KEY": "@google_api_key"
  },
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  }
}
